{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "oz",
      "camelcase": "Oz",
      "title": "Oz",
      "scope": "source.oz",
      "file-types": [
        "oz"
      ],
      "injection-regex": "^oz$",
      "class-name": "TreeSitterOz",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A tree-sitter parser for the Oz programming language.",
    "authors": [
      {
        "name": "Cynthia Allen",
        "email": "cynthiaallen228@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/scrmbld/tree-sitter-oz"
    },
    "highlights": "queries/highlights.scm"
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
